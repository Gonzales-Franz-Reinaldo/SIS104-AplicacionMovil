<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".ejercicios.EjerciciosActivity">

    <Button
        android:id="@+id/buttonEcuacion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/menu_ecuacion" />

    <Button
        android:id="@+id/buttonComplejos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/menu_complejos" />

    <Button
        android:id="@+id/buttonSumaModel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SUMA MODEL" />

    <Button
        android:id="@+id/buttonMultiplicacionModel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MULTIPLICACION MODEL" />

    <Button
        android:id="@+id/buttonDivisionModel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DIVISION MODEL" />

    <Button
        android:id="@+id/salirEjercicios"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/menu_salir" />
</LinearLayout>